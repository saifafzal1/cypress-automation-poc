{
  "name": "cypress-automation-poc",
  "version": "1.0.0",
  "description": "Cypress 15 Automation POC with POM, Docker, Jenkins (Parallel) and GitHub Actions",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:smoke": "cypress run --spec 'cypress/e2e/smoke/**'",
    "cy:sanity": "cypress run --spec 'cypress/e2e/sanity/**'",
    "cy:regression": "cypress run --spec 'cypress/e2e/regression/**'",
    "cy:regression:auth": "cypress run --spec 'cypress/e2e/regression/authentication/**'",
    "cy:regression:appointment": "cypress run --spec 'cypress/e2e/regression/appointment/**'",
    "cy:regression:confirmation": "cypress run --spec 'cypress/e2e/regression/confirmation/**'",
    "cy:regression:history": "cypress run --spec 'cypress/e2e/regression/history/**'",
    "cy:regression:api": "cypress run --spec 'cypress/e2e/regression/api-validation/**'",
    "cy:all": "cypress run",
    "cy:todo:smoke": "cypress run --spec 'cypress/e2e/todo-app/smoke/**'",
    "cy:todo:regression": "cypress run --spec 'cypress/e2e/todo-app/regression/**'",
    "cy:todo:regression:crud": "cypress run --spec 'cypress/e2e/todo-app/regression/crud/**'",
    "cy:todo:regression:filtering": "cypress run --spec 'cypress/e2e/todo-app/regression/filtering/**'",
    "cy:todo:regression:persistence": "cypress run --spec 'cypress/e2e/todo-app/regression/persistence/**'",
    "cy:todo:regression:bulk": "cypress run --spec 'cypress/e2e/todo-app/regression/bulk-operations/**'",
    "cy:todo:regression:edge": "cypress run --spec 'cypress/e2e/todo-app/regression/edge-cases/**'",
    "cy:todo:all": "cypress run --spec 'cypress/e2e/todo-app/**'",
    "report:merge": "mochawesome-merge mochawesome-temp/*.json -o mochawesome-report/merged.json",
    "report:generate": "marge mochawesome-report/merged.json -f report -o mochawesome-report",
    "report": "npm run report:merge && npm run report:generate",
    "report:full": "./scripts/generate-report.sh",
    "report:clean": "rm -rf mochawesome-temp mochawesome-report",
    "precy:all": "npm run report:clean",
    "precy:smoke": "npm run report:clean",
    "precy:regression": "npm run report:clean"
  },
  "keywords": ["cypress", "automation", "pom", "jenkins", "docker"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^15.10.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.3.2"
  }
}
